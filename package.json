{
	"name": "waddler",
	"version": "0.0.12",
	"type": "module",
	"repository": "https://github.com/drizzle-team/waddler",
	"license": "MIT",
	"scripts": {
		"start": "npx tsx ./src/dev/test.ts",
		"start:db": "npx tsx ./src/dev/db-test.ts ",
		"test": "vitest --config ./vitest.config.ts",
		"lint": "concurrently -n eslint,dprint \"eslint --ext ts .\" \"dprint check --list-different\"",
		"build": "pnpm tsx scripts/build.ts",
		"pack": "(cd dist && npm pack --pack-destination ..) && rm -f package.tgz && mv *.tgz package.tgz"
	},
	"exports": {
		".": {
			"import": {
				"types": "./index.d.ts",
				"default": "./index.js"
			},
			"require": {
				"types": "./index.d.cts",
				"default": "./index.cjs"
			}
		},
		"./duckdb": {
			"import": {
				"types": "./duckdb/index.d.ts",
				"default": "./duckdb/index.js"
			},
			"require": {
				"types": "./duckdb/index.d.cts",
				"default": "./duckdb/index.cjs"
			}
		},
		"./duckdb-neo": {
			"import": {
				"types": "./duckdb-neo/index.d.ts",
				"default": "./duckdb-neo/index.js"
			},
			"require": {
				"types": "./duckdb-neo/index.d.cts",
				"default": "./duckdb-neo/index.cjs"
			}
		},
		"./node-postgres": {
			"import": {
				"types": "./node-postgres/index.d.ts",
				"default": "./node-postgres/index.js"
			},
			"require": {
				"types": "./node-postgres/index.d.cts",
				"default": "./node-postgres/index.cjs"
			}
		}
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.16.4",
		"@duckdb/node-api": "1.1.2-alpha.4",
		"@eslint/compat": "^1.2.7",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.23.0",
		"@types/dockerode": "^3.3.35",
		"@types/node": "^22.7.6",
		"@types/pg": "^8.11.11",
		"@typescript-eslint/eslint-plugin": "^8.28.0",
		"concurrently": "^8.2.1",
		"dockerode": "^4.0.4",
		"dprint": "^0.46.2",
		"duckdb": "^1.1.1",
		"eslint": "^9.23.0",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-no-instanceof": "^1.0.1",
		"eslint-plugin-unicorn": "^56.0.1",
		"eslint-plugin-unused-imports": "^4.1.4",
		"get-port": "^7.1.0",
		"tsup": "^8.4.0",
		"tsx": "^4.19.1",
		"typescript": "^5.5.4",
		"vitest": "^3.0.9",
		"zx": "^8.1.9"
	},
	"peerDependencies": {
		"@duckdb/node-api": "^1.1.2-alpha.4",
		"duckdb": "^1.1.1",
		"pg": "^8.14.0",
		"pg-query-stream": "^4.8.0",
		"postgres": "^3.4.5"
	},
	"peerDependenciesMeta": {
		"duckdb": {
			"optional": true
		},
		"@duckdb/node-api": {
			"optional": true
		},
		"pg": {
			"optional": true
		},
		"pg-query-stream": {
			"optional": true
		},
		"postgres": {
			"optional": true
		}
	}
}
